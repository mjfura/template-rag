   "source": [
    "#!/usr/bin/env python\n",
    "# coding: utf-8\n",
    "\n",
    "# # Instalamos dependencias:\n",
    "\n",
    "# In[2]:\n",
    "\n",
    "get_ipython().system('pip install langchain langchain_community spacy PyPDF2 sentence_transformers qdrant-client')\n",
    "\n",
    "\n",
    "# In[3]:\n",
    "\n",
    "import spacy\n",
    "get_ipython().system('python -m spacy download en_core_web_sm')\n",
    "\n",
    "\n",
    "# In[4]:\n",
    "# Instalamos las dependencias necesarias\n",
    "get_ipython().system('pip install langchain==0.0.123 langchain_community==0.0.10 spacy==3.5.0 PyPDF2==3.0.1 sentence-transformers==2.2.2 scikit-learn==1.2.2 qdrant-client==1.1.5')\n",
    "\n",
    "\n",
    "# In[5]:\n",
    "# Instalamos Streamlit (opcional si quieres probar la aplicación al final)\n",
    "get_ipython().system('pip install streamlit==1.22.0')\n",
    "\n",
    "\n",
    "# In[6]:\n",
    "import spacy\n",
    "get_ipython().system('python -m spacy download en_core_web_sm')\n",
    "\n",
    "\n",
    "# # Carga de Documentos\n",
    "\n",
    "# In[36]:\n",
    "\n",
    "from langchain.document_loaders import PyPDFLoader\n",
    "import os\n",
    "\n",
    "# Ruta al archivo PDF\n",
    "#file_path = \"./Meat products_ sell them legally in England - GOV.UK.pdf\"\n",
    "file_path = \"./Regulaciones cacao y chocolate 2003.pdf\"\n",
    "\n",
    "# Verificamos que el archivo exista\n",
    "assert os.path.exists(file_path), \"El archivo PDF no se encuentra en la ruta especificada.\"\n",
    "\n",
    "# Cargamos el documento\n",
    "loader = PyPDFLoader(file_path)\n",
    "documents = loader.load()\n",
    "\n",
    "print(f\"Se han cargado {len(documents)} páginas del documento PDF.\")\n",
    "\n",
    "\n",
    "# In[37]:\n",
    "\n",
    "import spacy\n",
    "\n",
    "# Cargamos el modelo de spaCy para inglés\n",
    "nlp = spacy.load(\"en_core_web_sm\")\n",
    "\n",
    "def preprocess_text(text):\n",
    "   doc = nlp(text)\n",
    "   tokens = []\n",
    "   for token in doc:\n",
    "       # Filtramos tokens que no sean stop words, signos de puntuación o números\n",
    "       if not token.is_stop and not token.is_punct and not token.like_num:\n",
    "           tokens.append(token.lemma_.lower())\n",
    "   return ' '.join(tokens)\n",
    "\n",
    "# Aplicamos la función de limpieza a cada documento\n",
    "cleaned_documents = []\n",
    "for doc in documents:\n",
    "   cleaned_text = preprocess_text(doc.page_content)\n",
    "   doc.page_content = cleaned_text\n",
    "   cleaned_documents.append(doc)\n",
    "\n",
    "print(\"Limpieza de datos completada.\")\n",
    "\n",
    "\n",
    "# In[38]:\n",
    "\n",
    "# Mostramos el contenido de la primera página después de la limpieza\n",
    "print(cleaned_documents[0].page_content[:500])\n",
    "\n",
    "\n",
    "# In[39]:\n",
    "\n",
    "def split_into_sentences(text):\n",
    "    doc = nlp(text)\n",
    "    sentences = [sent.text.strip() for sent in doc.sents if sent.text.strip()]\n",
    "    return sentences\n",
    "\n",
    "# Obtenemos todas las oraciones de todos los documentos\n",
    "all_sentences = []\n",
    "for doc in cleaned_documents:\n",
    "    sentences = split_into_sentences(doc.page_content)\n",
    "    all_sentences.extend(sentences)\n",
    "\n",
    "print(f\"Se han obtenido {len(all_sentences)} oraciones en total.\")\n",
    "\n",
    "\n",
    "# In[40]:\n",
    "\n",
    "print(all_sentences)\n",
    "\n",
    "\n",
    "# In[41]:\n",
    "\n",
    "from sentence_transformers import SentenceTransformer\n",
    "\n",
    "# Cargamos el modelo de embeddings\n",
    "model = SentenceTransformer('all-MiniLM-L6-v2')\n",
    "\n",
    "# Calculamos los embeddings de las oraciones\n",
    "sentence_embeddings = model.encode(all_sentences)\n",
    "\n",
    "print(\"Embeddings de las oraciones calculados.\")\n",
    "\n",
    "\n",
    "# In[42]:\n",
    "\n",
    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "import numpy as np\n",
    "\n",
    "# Calculamos las distancias de coseno entre oraciones\n",
    "cosine_distances = []\n",
    "for i in range(len(sentence_embeddings) - 1):\n",
    "    similarity = cosine_similarity(\n",
    "        [sentence_embeddings[i]],\n",
    "        [sentence_embeddings[i + 1]]\n",
    "    )[0][0]\n",
    "    distance = 1 - similarity\n",
    "    cosine_distances.append(distance)\n",
    "\n",
    "print(\"Distancias entre oraciones calculadas.\")\n",
    "\n",
    "\n",
    "# In[43]:\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Graficamos las distancias de coseno\n",
    "plt.plot(cosine_distances)\n",
    "y_upper_bound = 0.9\n",
    "plt.ylim(0, y_upper_bound)\n",
    "plt.xlim(0, len(cosine_distances))\n",
    "\n",
    "# Establecemos el umbral como el percentil 95 de las distancias\n",
    "breakpoint_percentile_threshold = 95\n",
    "breakpoint_distance_threshold = np.percentile(cosine_distances, breakpoint_percentile_threshold)\n",
    "plt.axhline(y=breakpoint_distance_threshold, color='r', linestyle='-')\n",
    "\n",
    "# Calculamos cuántas distancias están por encima del umbral\n",
    "num_distances_above_threshold = len([x for x in cosine_distances if x > breakpoint_distance_threshold])\n",
    "plt.text(x=(len(cosine_distances) * 0.01), y=y_upper_bound / 50, s=f\"{num_distances_above_threshold + 1} Chunks\")\n",
    "\n",
    "# Obtener los índices de los puntos de quiebre\n",
    "indices_above_thresh = [i for i, x in enumerate(cosine_distances) if x > breakpoint_distance_threshold]\n",
    "\n",
    "# Graficamos las regiones de chunking\n",
    "colors = ['b', 'g', 'r', 'c', 'm', 'y', 'k']\n",
    "for i, breakpoint_index in enumerate(indices_above_thresh):\n",
    "    start_index = 0 if i == 0 else indices_above_thresh[i - 1]\n",
    "    end_index = breakpoint_index if i < len(indices_above_thresh) - 1 else len(cosine_distances)\n",
    "\n",
    "    plt.axvspan(start_index, end_index, facecolor=colors[i % len(colors)], alpha=0.25)\n",
    "    plt.text(x=np.average([start_index, end_index]),\n",
    "             y=breakpoint_distance_threshold + (y_upper_bound) / 20,\n",
    "             s=f\"Chunk #{i}\", horizontalalignment='center',\n",
    "             rotation='vertical')\n",
    "\n",
    "# Sombras desde el último punto de quiebre hasta el final\n",
    "if indices_above_thresh:\n",
    "    last_breakpoint = indices_above_thresh[-1]\n",
    "    if last_breakpoint < len(cosine_distances):\n",
    "        plt.axvspan(last_breakpoint, len(cosine_distances), facecolor=colors[len(indices_above_thresh) % len(colors)], alpha=0.25)\n",
    "        plt.text(x=np.average([last_breakpoint, len(cosine_distances)]),\n",
    "                 y=breakpoint_distance_threshold + (y_upper_bound) / 20,\n",
    "                 s=f\"Chunk #{i + 1}\",\n",
    "                 rotation='vertical')\n",
    "\n",
    "plt.title(\"Chunks Basados en Distancias de Coseno entre Oraciones\")\n",
    "plt.xlabel(\"Índice de Oraciones en el Documento\")\n",
    "plt.ylabel(\"Distancia de Coseno entre Oraciones Secuenciales\")\n",
    "plt.show()\n",
    "\n",
    "\n",
    "# In[44]:\n",
    "\n",
    "# Identificamos los puntos de quiebre\n",
    "breakpoints = [i + 1 for i, d in enumerate(cosine_distances) if d > breakpoint_distance_threshold]\n",
    "\n",
    "print(f\"Se han identificado {len(breakpoints)} puntos de quiebre.\")\n",
    "\n",
    "\n",
    "# In[45]:\n",
    "\n",
    "chunks = []\n",
    "start = 0\n",
    "for breakpoint in breakpoints:\n",
    "    chunk = ' '.join(all_sentences[start:breakpoint])\n",
    "    chunks.append(chunk)\n",
    "    start = breakpoint\n",
    "\n",
    "# Agregamos el último chunk\n",
    "if start < len(all_sentences):\n",
    "    chunk = ' '.join(all_sentences[start:])\n",
    "    chunks.append(chunk)\n",
    "\n",
    "print(f\"Se han creado {len(chunks)} chunks.\")\n",
    "\n",
    "\n",
    "# In[46]:\n",
    "\n",
    "# Mostramos el contenido del primer chunk\n",
    "print(chunks[0][:500])\n",
    "\n",
    "\n",
    "# In[47]:\n",
    "\n",
    "import os\n",
    "\n",
    "os.makedirs('data/chunks', exist_ok=True)\n",
    "\n",
    "for i, chunk in enumerate(chunks):\n",
    "    with open(f'data/chunks/chunk_{i}.txt', 'w', encoding='utf-8') as f:\n",
    "        f.write(chunk)\n",
    "\n",
    "print(\"Chunks guardados en la carpeta 'data/chunks'.\")\n",
    "\n",
    "\n",
    "# In[48]:\n",
    "\n",
    "# Calculamos los embeddings de los chunks\n",
    "chunk_embeddings = model.encode(chunks)\n",
    "\n",
    "print(\"Embeddings de los chunks calculados.\")\n",
    "\n",
    "\n",
    "# In[49]:\n",
    "\n",
    "import pickle\n",
    "\n",
    "os.makedirs('embeddings', exist_ok=True)\n",
    "\n",
    "# Guardamos los embeddings\n",
    "with open('embeddings/chunk_embeddings.pkl', 'wb') as f:\n",
    "    pickle.dump(chunk_embeddings, f)\n",
    "\n",
    "# Guardamos los chunks\n",
    "with open('embeddings/chunk_texts.pkl', 'wb') as f:\n",
    "    pickle.dump(chunks, f)\n",
    "\n",
    "print(\"Embeddings y chunks guardados en la carpeta 'embeddings'.\")\n",
    "\n",
    "\n",
    "# In[50]:\n",
    "\n",
    "from qdrant_client import QdrantClient\n",
    "from qdrant_client.http.models import Distance, VectorParams\n",
    "\n",
    "# Inicializamos el cliente de Qdrant (en memoria)\n",
    "qdrant_client = QdrantClient(\":memory:\")\n",
    "\n",
    "collection_name = 'uk_regulations'\n",
    "\n",
    "# Obtenemos el tamaño de los vectores\n",
    "vector_size = chunk_embeddings.shape[1]\n",
    "\n",
    "# Creamos la colección en Qdrant\n",
    "qdrant_client.recreate_collection(\n",
    "    collection_name=collection_name,\n",
    "    vectors_config=VectorParams(size=vector_size, distance=Distance.COSINE),\n",
    ")\n",
    "\n",
    "# Preparamos los datos para cargar en Qdrant\n",
    "payload = [{'chunk': chunk} for chunk in chunks]\n",
    "ids = list(range(len(chunks)))\n",
    "\n",
    "# Subimos los datos a Qdrant\n",
    "qdrant_client.upload_collection(\n",
    "    collection_name=collection_name,\n",
    "    vectors=chunk_embeddings,\n",
    "    payload=payload,\n",
    "    ids=ids,\n",
    ")\n",
    "\n",
    "print(\"Embeddings indexados en Qdrant.\")\n",
    "\n",
    "\n",
    "# In[51]:\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "def search_chunks(query, top_k=5):\n",
    "    # Calculamos el embedding de la consulta\n",
    "    query_embedding = model.encode([query])[0]\n",
    "    # Calculamos las similitudes\n",
    "    similarities = cosine_similarity([query_embedding], chunk_embeddings)[0]\n",
    "    # Obtenemos los índices de los chunks más similares\n",
    "    indices = np.argsort(similarities)[::-1][:top_k]\n",
    "    # Mostramos los resultados\n",
    "    for idx in indices:\n",
    "        print(f\"Resultado {idx + 1} (Score: {similarities[idx]:.4f}):\")\n",
    "        print(chunks[idx])\n",
    "        print(\"---\")\n",
    "\n",
    "# Ejemplo de consulta\n",
    "query = \"requirements for labeling meat products\"\n",
    "search_chunks(query)\n",
    "\n",
    "\n",
    "# In[52]:\n",
    "\n",
    "def search_chunks_qdrant(query, top_k=5):\n",
    "    query_embedding = model.encode([query])[0]\n",
    "    search_result = qdrant_client.search(\n",
    "        collection_name=collection_name,\n",
    "        query_vector=query_embedding.tolist(),\n",
    "        limit=top_k\n",
    "    )\n",
    "    # Mostramos los resultados\n",
    "    for hit in search_result:\n",
    "        print(f\"Resultado con score {hit.score:.4f}:\")\n",
    "        print(hit.payload['chunk'])\n",
    "        print(\"---\")\n",
    "\n",
    "# Ejemplo de consulta\n",
    "query = \"requirements for labeling meat products\"\n",
    "search_chunks_qdrant(query)\n"
   ]
